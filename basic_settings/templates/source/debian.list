<% if @non_free -%>
deb <%= @os_url %> <%= @os_name %> main contrib non-free non-free-firmware
deb-src <%= @os_url %> <%= @os_name %> main contrib non-free non-free-firmware
<% else -%>
deb <%= @os_url %> <%= @os_name %> main non-free-firmware
deb-src <%= @os_url %> <%= @os_name %> main non-free-firmware
<% end -%>
deb <%= @os_url_security %> <%= @os_name %>-security main non-free-firmware
deb-src <%= @os_url_security %> <%= @os_name %>-security main non-free-firmware
deb <%= @os_url %> <%= @os_name %>-updates main non-free-firmware
deb-src <%= @os_url %> <%= @os_name %>-updates main non-free-firmware