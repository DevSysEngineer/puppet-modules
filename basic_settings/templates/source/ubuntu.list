<% if @non_free -%>
deb <%= @os_url %> <%= @os_name %> main universe restricted multiverse
deb-src <%= @os_url %> <%= @os_name %> main universe restricted multiverse
deb <%= @os_url_security %> <%= @os_name %>-security main universe restricted multiverse
deb-src <%= @os_url_security %> <%= @os_name %>-security main universe restricted multiverse
deb <%= @os_url %> <%= @os_name %>-updates main universe restricted multiverse
deb-src <%= @os_url %> <%= @os_name %>-updates main universe restricted multiverse
<% else -%>
deb <%= @os_url %> <%= @os_name %> main universe restricted
deb-src <%= @os_url %> <%= @os_name %> main universe restricted
deb <%= @os_url_security %> <%= @os_name %>-security main universe restricted 
deb-src <%= @os_url_security %> <%= @os_name %>-security main universe restricted
deb <%= @os_url %> <%= @os_name %>-updates main universe restricted
deb-src <%= @os_url %> <%= @os_name %>-updates main universe restricted
<% end -%>