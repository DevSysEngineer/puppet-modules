# Managed by puppet
<% if @non_free -%>
deb <%= @os_url %> <%= @os_name %> <%= @os_repo %> restricted multiverse
deb-src <%= @os_url %> <%= @os_name %> <%= @os_repo %> restricted multiverse
deb <%= @os_url_security %> <%= @os_name %>-security <%= @os_repo %> restricted multiverse
deb-src <%= @os_url_security %> <%= @os_name %>-security <%= @os_repo %> restricted multiverse
deb <%= @os_url %> <%= @os_name %>-updates <%= @os_repo %> restricted multiverse
deb-src <%= @os_url %> <%= @os_name %>-updates <%= @os_repo %> restricted multiverse
<% else -%>
deb <%= @os_url %> <%= @os_name %> <%= @os_repo %> main universe
deb-src <%= @os_url %> <%= @os_name %> <%= @os_repo %> main universe
deb <%= @os_url_security %> <%= @os_name %>-security <%= @os_repo %> main universe
deb-src <%= @os_url_security %> <%= @os_name %>-security <%= @os_repo %> main universe
deb <%= @os_url %> <%= @os_name %>-updates <%= @os_repo %> main universe
deb-src <%= @os_url %> <%= @os_name %>-updates <%= @os_repo %> main universe
<% end -%>