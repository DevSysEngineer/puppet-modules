# Managed by puppet

<% if ! @rules.empty? -%>
# Custom rules

<% @rules.each do |rule| -%>
<%= rule %>
<% end -%>
<% end -%>
<% if ! @rule_suspicious_packages.empty? -%>

# Suspicious activity
<% @rule_suspicious_packages.each do |rule_suspicious_package| -%>
<%= rule_suspicious_package %> -p x -k susp_activity
<% end -%>
<% end -%>